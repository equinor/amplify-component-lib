  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/amplify-component-lib.git"
  },
  "bugs": {
    "url": "https://github.com/equinor/amplify-component-lib/issues"
  },
  "files": [
    "dist/*"
