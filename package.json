{
  "name": "@equinor/amplify-components",
  "version": "3.2.10",
  "description": "Frontend Typescript components for the Amplify team",
  "files": [
    "dist/*"
  ],
  "main": "dist/index.js",
  "publishConfig": {
    "main": "dist"
  },
  "types": "dist/types/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "sideEffects": false,
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006 -s public",
    "build-storybook": "NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=2048' build-storybook",
    "setup": "yarn",
    "pretty": "prettier --check ./src",
    "pretty:fix": "prettier --write ./src",
    "lint": "tsc && eslint ./src --ext .jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .jsx,.ts,.tsx --fix",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "setup-husky": "husky install",
    "precommit": "yarn && node tooling/commit-tasks.mjs",
    "build": "yarn build-components",
    "build-components": "rollup -c rollup.config.mjs && tsc -p tsconfig.build.json",
    "build-components:ci": "rm -rf lib dist && rollup -c rollup.config.mjs && tsc -p tsconfig.build.json",
    "version": "version-bump cli --bump",
    "postversion": "git add package.json && git commit -m 'Upgraded package version'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "2.30.0",
    "@azure/msal-react": "1.4.9",
    "@microsoft/signalr": "^7.0.2",
    "html2canvas": "^1.4.1",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.3",
    "proj4": "^2.8.1",
    "random-seed": "^0.3.0",
    "react-dropzone": "^14.2.3",
    "react-transition-group": "^4.4.5",
    "uuid": "^9.0.0"
  },
  "resolutions": {
    "@azure/msal-common": "7.6.0"
  },
  "devDependencies": {
    "@equinor/eds-core-react": "^0.28.0",
    "@faker-js/faker": "^7.6.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@theo.gravity/version-bump": "^2.0.14",
    "@types/leaflet": "^1.9.0",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/proj4": "^2.5.2",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^21.1.0",
    "ora": "^6.1.2",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^3.12.0",
    "rollup-plugin-delete": "^2.0.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.3"
  },
  "peerDependencies": {
    "@equinor/eds-core-react": "0.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/amplify-components.git"
  },
  "author": "Amplify Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/equinor/amplify-components/issues"
  },
  "homepage": "https://github.com/equinor/amplify-components#readme",
  "packageManager": "yarn@3.2.4"
}
