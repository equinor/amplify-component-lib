{
  "name": "@equinor/amplify-components",
  "version": "5.0.0",
  "description": "Frontend Typescript components for the Amplify team",
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Amplify Team",
  "license": "ISC",
  "homepage": "https://github.com/equinor/amplify-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/amplify-components.git"
  },
  "bugs": {
    "url": "https://github.com/equinor/amplify-components/issues"
  },
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider && storybook dev -p 6006",
    "build-storybook": "NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=2048' storybook build",
    "setup": "yarn",
    "pretty": "prettier --check ./config ./src",
    "pretty:fix": "prettier --write ./config ./src",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "setup-husky": "husky install",
    "precommit": "yarn && node tooling/commit-tasks.mjs",
    "build": "yarn build-components",
    "build-components": "rollup -c && tsc --outDir dist --declarationDir dist/types --declaration true --emitDeclarationOnly --project tsconfig.build.json",
    "build-components:ci": "rm -rf lib dist && yarn build-components",
    "build-and-use": "node tooling/build-and-use.mjs",
    "version": "version-bump cli --bump",
    "postversion": "git add package.json && git commit -m 'Upgraded package version'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "2.38.0",
    "@azure/msal-react": "1.5.9",
    "@microsoft/signalr": "^7.0.11",
    "html2canvas": "^1.4.1",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.4",
    "proj4": "^2.9.0",
    "random-seed": "^0.3.0",
    "react-dropzone": "^14.2.3",
    "react-transition-group": "^4.4.5",
    "uuid": "^9.0.1"
  },
  "resolutions": {
    "@azure/msal-common": "7.6.0"
  },
  "devDependencies": {
    "@equinor/eds-core-react": "0.33.0",
    "@faker-js/faker": "^8.1.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-actions": "^7.4.5",
    "@storybook/addon-docs": "^7.4.5",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addons": "^7.4.5",
    "@storybook/node-logger": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.4.5",
    "@tanstack/react-query": "^4.35.7",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@theo.gravity/version-bump": "^2.0.14",
    "@types/leaflet": "^1.9.6",
    "@types/lodash-es": "^4.17.9",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.2",
    "@types/proj4": "^2.5.3",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.7",
    "@types/styled-components": "^5.1.28",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.14",
    "jest-styled-components": "^7.1.1",
    "jsdom": "^22.1.0",
    "ora": "^7.0.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^3.27.0",
    "rollup-plugin-delete": "^2.0.0",
    "storybook": "^7.4.5",
    "storybook-addon-react-router-v6": "^2.0.7",
    "styled-components": "^6.0.8",
    "typescript": "^5.2.2",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@equinor/eds-core-react": "^0.31.1",
    "@tanstack/react-query": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "styled-components": "^5.3.10"
  },
  "packageManager": "yarn@3.2.4"
}
