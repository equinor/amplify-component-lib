{
  "name": "@equinor/amplify-components",
  "version": "2.2.21",
  "description": "Frontend Typescript components for the Amplify team",
  "files": [
    "dist/*"
  ],
  "main": "dist/amplify-components.cjs.js",
  "publishConfig": {
    "main": "dist/amplify-components.cjs.js",
    "module": "dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "sideEffects": false,
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "setup": "yarn",
    "pretty": "prettier --check ./src",
    "pretty:fix": "prettier --write ./src",
    "lint": "tsc && eslint ./src --ext .jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .jsx,.ts,.tsx --fix",
    "test": "run-script-os",
    "test:win32": "set CI=true&react-scripts test --colors",
    "test:macos": "CI=true react-scripts test",
    "test:watch": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "setup-husky": "husky install",
    "precommit": "yarn && node tooling/commit-tasks.mjs",
    "build": "yarn build-components",
    "build-components": "rollup -c rollup.config.js && tsc -p tsconfig.build.json",
    "build-components:ci": "rm -rf lib dist && rollup -c rollup.config.js && tsc -p tsconfig.build.json",
    "version": "version-bump cli --bump",
    "postversion": "git add package.json && git commit -m 'Upgraded package version'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "2.30.0",
    "@azure/msal-react": "1.4.9",
    "html2canvas": "^1.4.1",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.3",
    "proj4": "^2.8.1",
    "random-seed": "^0.3.0",
    "react-dropzone": "^14.2.3",
    "react-transition-group": "^4.4.5",
    "uuid": "^9.0.0"
  },
  "resolutions": {
    "nth-check": "2.1.1",
    "glob-parent": "6.0.2",
    "trim": "1.0.1",
    "trim-newlines": "4.0.2",
    "@azure/msal-common": "7.6.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/runtime": "^7.20.7",
    "@equinor/eds-core-react": "^0.28.0",
    "@faker-js/faker": "^7.6.0",
    "@open-wc/building-rollup": "^2.2.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.15",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@theo.gravity/version-bump": "^2.0.14",
    "@types/jest": "^29.2.5",
    "@types/leaflet": "^1.9.0",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/proj4": "^2.5.2",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "audit-ci": "^6.6.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-preset-minify": "^0.5.2",
    "chalk": "^5.2.0",
    "child_process": "^1.0.2",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "husky": "^8.0.3",
    "ora": "^6.1.2",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.79.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "run-script-os": "^1.1.6",
    "styled-components": "^5.3.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    }
  },
  "peerDependencies": {
    "@equinor/eds-core-react": "0.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "webpack": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/amplify-components.git"
  },
  "author": "Amplify Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/equinor/amplify-components/issues"
  },
  "homepage": "https://github.com/equinor/amplify-components#readme",
  "packageManager": "yarn@3.2.4"
}
